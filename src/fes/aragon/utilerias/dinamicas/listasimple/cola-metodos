package fes.aragon.utilerias.dinamicas.cola;

import fes.aragon.utilerias.dinamicas.listasimple.ListaSimple;

public class Cola<E> extends ListaSimple<E> {
    public void borrar() {
        while (!esVacia()) {
            eliminarEnCabeza();
        }
    }

    public boolean esVacia() {
        return super.esVacia();
    }

    public void insertar(E dato) {
        agregarEnCola(dato);
    }

    public E extraer() {
        E dato = obtenerCabeza();
        eliminarEnCabeza();
        return dato;
    }

    public E primerElemento() {
        return obtenerCabeza();
    }
}
